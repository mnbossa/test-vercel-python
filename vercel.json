{
  "routes": [
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/app.js", "dest": "/public/app.js" },
    { "src": "/api/titles", "dest": "/api/titles.py" },
    { "src": "/api/(.*)", "dest": "/api/proxy.py" }
  ],
  "rewrites": [
    { "source": "/titles", "destination": "/api/titles" }
  ]
}
